<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.littlebuddha.housekeeping.mapper.system.OperatorRoleMapper">
    <select id="findByOperator" resultType="String">
        SELECT role_id FROM sys_operator_role
        WHERE operator_id = #{id}
    </select>
</mapper>